<?xml version='1.0' encoding='utf-8'?>
<plugin id="com.chinact.mobile.plugin.app" version="1.0.0" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
	<name>App</name>
	<js-module name="App" src="www/App.js">
		<clobbers target="navigator.app" />
	</js-module>
	<platform name="android">
		<config-file parent="/*" target="res/xml/config.xml">
			<feature name="App">
				<param name="android-package" value="com.chinact.mobile.plugin.app.App" />
			</feature>
		</config-file>
		<config-file parent="/*" target="AndroidManifest.xml">
			<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
		</config-file>
		<source-file src="src/android/App.java" target-dir="src/com/chinact/mobile/plugin/app" />
	</platform>
</plugin>